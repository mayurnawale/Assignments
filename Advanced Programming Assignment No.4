def fact_of_fact(in_num):
    def get_factorial(n):
        if n == 1:
            return 1
        else:
            return n*get_factorial(n-1)      
    out_num = 1
    for ele in range(1,in_num+1):
        out_num *= get_factorial(ele)
    print(f'fact_of_fact({in_num}) ➞ {out_num}')
fact_of_fact(4)
fact_of_fact(5)
fact_of_fact(6)