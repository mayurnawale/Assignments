def uncensor(in_string,in_vowels):
    window = 0
    out_string = ''
    for ele in in_string:
        if ele == '*':
            out_string += in_vowels[window]
            window +=1
        else:
            out_string += ele
    print(f'uncensor{in_string,in_vowels} ➞ {out_string}')
uncensor("Wh*r* d*d my v*w*ls g*?", "eeioeo")
uncensor("abcd", "")
uncensor("*PP*RC*S*", "UEAE")

