def fib_fast_one(in_num):
    back_two,back_one,output = 0,1,0
    for ele in range(in_num+1):
        if ele > 1:
            output = back_two+back_one
            back_two = back_one
            back_one = output
    print(f'fib_fast_two({in_num}) ➞ {output}')
fib_fast_one(5)
fib_fast_one(10)
fib_fast_one(20)
fib_fast_one(50)

